# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User{
  id: ID!
  userName: String!
  email: String!
  products: [Product!]!
}

type Product{
  id: ID!
  name: String!
  description: String!
  user: User!
}

type Query{
  products:[Product!]!
}

input NewProduct{
  name: String!
  description: String!
}

type Mutation{
  createProduct(input: NewProduct!) : Product!
}